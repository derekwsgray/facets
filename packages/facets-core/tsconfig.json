{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "moduleResolution": "bundler",
        "outDir": "./dist",
        "strict": true,
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "downlevelIteration": true,
        "removeComments": false,
        "isolatedModules": false,
        "noEmit": false
    },
    "include": [
        "src/**/*"
    ]
}
